<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.2.xsd">
	
	<bean name="fileWriter" class="br.com.springIntro.sectionFour.FileWriter"/>

	<bean name="consoleWriter" class="br.com.springIntro.sectionFour.ConsoleWriter"/>
	
	<!-- Using @Qualifier -->
	<bean name="anotherConsoleWriter" class="br.com.springIntro.sectionFour.ConsoleWriter"/>
		
	<bean name="logger" class="br.com.springIntro.sectionFour.Logger"/>
	
	<context:annotation-config/>
</beans>
